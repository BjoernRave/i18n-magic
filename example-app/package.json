    "i18n:debug": "DEBUG_NAMESPACE_MATCHING=true node ../dist/cli.js scan"
  },
  "dependencies": {
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "react": "^18.0.0",
    "typescript": "^5.0.0"
  }
}
