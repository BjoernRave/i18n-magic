{
  "name": "i18n-magic",
  "version": "0.9.5",
  "license": "MIT",
  "description": "CLI to help you manage your locales JSON with translations, replacements, etc. with OpenAI",
  "repository": {
    "type": "git",
    "url": "https://github.com/BjoernRave/i18n-magic.git"
  },
  "author": "BjoernRave",
  "main": "dist/index.js",
  "module": "dist/i18n-magic.esm.js",
  "typings": "dist/index.d.ts",
  "bin": "dist/index.js",
  "files": ["dist", "src"],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build && echo '#!/usr/bin/env node' | cat - dist/index.js > temp && mv temp dist/index.js",
    "lint": "dts lint",
    "prepare": "dts build && echo '#!/usr/bin/env node' | cat - dist/index.js > temp && mv temp dist/index.js",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "fast-glob": "^3.3.2",
    "i18next-scanner": "^4.6.0",
    "openai": "^4.72.0",
    "prompts": "^2.4.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tsconfig/recommended": "^1.0.8",
    "@types/node": "^20.12.12",
    "@types/prompts": "^2.4.9",
    "dts-cli": "^2.0.5",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3"
  }
}
